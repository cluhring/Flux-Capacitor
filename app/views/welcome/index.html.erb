<div class="row">
  <div class="container routing col-md-2">
    <div class='address-search' id="address1">
      <%= form_tag("/welcome", method: "get") do %>
      <%= label_tag("Trip Planner:") %><br><br>
      <%= text_field_tag(:map_address1, "Starting Address") %><br><br>
      <!-- <%= label_tag("Ending Point") %><br> -->
      <%= text_field_tag(:map_address2, "Ending Address") %><br><br>
      <button id="aa" class="btn btn-default">Add Another Address</button>
      <button type="submit" class="btn btn-default">Map It Out!</button>
      <% end %>
    </div>
    <div id='logo'>
      <%= image_tag "green_car5.jpg" %>
    </div>
  </div>
  <div class="col-md-10">
    <div class='map' id='map'>
    </div>
  </div>
</div>
